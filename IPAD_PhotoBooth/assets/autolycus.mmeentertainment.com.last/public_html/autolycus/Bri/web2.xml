<?xml version="1.0"?>
<WebConfig version="1">

<add key="Server" value="autolycus.mmeentertainment.com" /> 

<add key="UseSendEmail" value="1" /> <!-- 0=hides the send email -->
<add key="EmailSubject" value="A friend would like to share with you a favorite photo!" /> <!-- email subject -->
<add key="EmailDefaultMessage" value="Check out my picture from Bri's amazing Sweet 16 Photobooth" /> <!-- default custom msg -->
<add key="EmailBody" value="" /> <!-- body of the email that gets sent from favorite photo (user cannot alter this) -->

<add key="FacebookLike" value="mmeink" /> <!-- fan page to like; blank=hide -->
<add key="UseShareFacebook" value="1" /> <!-- hides the share button -->
<add key="FacebookTitle" value="Check out my picture from Bri's amazing Sweet 16 photobooth!"  
 /> <!-- title of the wall post -->
<add key="FacebookDesc" value="Want to find out about amazing events?  visit us at http://mmeink.com" /> <!-- facebook post desc -->

<add key="UseTweet" value="1" /> <!-- hides the tweet button -->
<add key="FollowTwitter" value="PhotomationUSA" /> <!-- twitter account to follow; blank=hide -->
<add key="TweetDefault" value="Check out my amazing picture from Bri's Sweet 16 Photobooth #PhotomationUSA" /> <!-- default tweet message (tinyurl auto-attached at end) -->


</WebConfig>